package Loop;

public class Question63 {

	public static void main(String[] args) 
	{
		int n=8;
	
		for(int i=1;i<=n;i++)
		{
	
			for(int j=1;j<=n;j++)
			{
			     if((i+j)%2==0)
			         System.out.print(" *");
			     if((i+j)%2==1)
			    	 System.out.print(" #");

			}
			System.out.println();
		}

	}

}
