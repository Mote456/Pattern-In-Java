package Loop;

public class complexPyramid {

	public static void main(String[] args)
	{
		int n=8;
		for(int i=1;i<=n;i++)
		{
			for(int j=i;j<=n;j++)
			{
				System.out.print(" ");
			}
			for(int k=1;k<=i*2-1;k++)
			{
				if(k%2==0)
				   System.out.print("#");
				else
				  System.out.print("*");
			}
			System.out.println();
		}

	}

}
